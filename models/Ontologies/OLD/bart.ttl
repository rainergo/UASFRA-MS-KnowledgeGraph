@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix : <http://example.org/foods#> .

# Person class
:Person a owl:Class ;
  rdfs:label "Person" .

:Peter a :Person ;
  rdfs:label "Peter" .

# Fruit class
:Fruit a owl:Class ;
  rdfs:label "Fruit" .

:Apple a :Fruit ;
  rdfs:label "Apple" .

# Eats relationship with additional properties
:eats a rdf:Property ;
  owl:domain :Person ;
  owl:range :Fruit .

:quickly a rdf:Property ;
  owl:domain :Activity ;
  rdfs:range rdf:Literal .

:withKnife a rdf:Property ;
  owl:domain :Activity ;
  rdfs:range rdf:Literal .

# Reified triple for eating an apple quickly
:eat-instance a :Activity ;
  :agent :Peter ;
  :object :Apple ;
  :property :quickly .

# Reified triple for eating an apple quickly and with a knife
:eat-with-knife-instance a :Activity ;
  :agent :Peter ;
  :object :Apple ;
  :property :quickly ;
  :property :withKnife .